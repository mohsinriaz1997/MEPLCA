@using CA.API.Models
<MudDialog class="modal-settings">
    <DialogContent>
        <MudContainer style="max-height:500px;overflow-y:scroll;">
                @if (Loading)
                {
                    <center><MudProgressCircular Color="Color.Primary" Size="Size.Large" Indeterminate="true" /></center>
                }
                @if (DialogFor == "ResourceMaster")
                {
                    <MudText Typo="Typo.h6">Detail</MudText>
                    <MudSpacer />
                    <MudDivider />
                    <MudGrid class="search-action-block align-items-center">
                        <MudItem xs="4" sm="4" md="4" lg="4">
                            <MudSelect xs="4" sm="4" md="4" lg="4" T="string" Label="Type" @bind-Value="oModelDetail.TypeOfResr" Variant="Variant.Outlined" AnchorOrigin="Origin.BottomCenter">
                                <MudSelectItem Value="@("Dyes")" />
                                <MudSelectItem Value="@("Mold")" />
                                <MudSelectItem Value="@("Tools")" />
                                <MudSelectItem Value="@("Gasoline")" />
                                <MudSelectItem Value="@("Machine")" />
                            </MudSelect>
                        </MudItem>
                        <MudItem xs="4" sm="4" md="4" lg="4">
                            <MudTextField InputMode="InputMode.text" MaxLength="200" Counter="200" HelperText="Enter From Resource Description" Immediate="true" Clearable="true" Label="Resource Description" Variant="Variant.Text"
                                      Adornment="Adornment.Start" AdornmentIcon="@Icons.Material.Filled.TextFields" @bind-Value="oModelDetail.ResrDescription" />
                        </MudItem>
                        <MudItem xs="3" sm="3" md="3" lg="3">
                            <MudTextField InputMode="InputMode.text" MaxLength="200" Counter="200" HelperText="Currency Code" Immediate="true" Clearable="true" Label="Currency Code" Variant="Variant.Text"
                                      Adornment="Adornment.Start" AdornmentIcon="@Icons.Material.Filled.TextFields" @bind-Value="oModelDetail.CurrCodeSap" Disabled />
                        </MudItem>
                        <MudItem xs="3" sm="3" md="3" lg="3">
                            <MudTextField InputMode="InputMode.text" MaxLength="200" Counter="200" HelperText="Currency" Immediate="true" Clearable="true" Label="Currency" Variant="Variant.Text"
                                      Adornment="Adornment.Start" AdornmentIcon="@Icons.Material.Filled.TextFields" @bind-Value="oModelDetail.CurrNameSap" Disabled />
                        </MudItem>
                        <MudItem xs="1" sm="1" md="1" lg="1" Class="text-right pr-5 pb-4">
                            <MudIconButton OnClick="@((e) => OpenSAPDataDialog(FullView))" Icon="@Icons.Material.Filled.MenuOpen" Color="Color.Primary" Size="Size.Small" Title="Search Item"></MudIconButton>
                        </MudItem>
                        <MudItem xs="4" sm="4" md="4" lg="4">
                            <MudNumericField InputMode="InputMode.numeric" Min="0" HelperText="Enter From Rate" Immediate="true" Clearable="true" Label="Rate" Variant="Variant.Text"
                                         Adornment="Adornment.Start" AdornmentIcon="@Icons.Material.Filled.Numbers" @bind-Value="oModelDetail.Rate" TextChanged="OnChange"></MudNumericField>
                        </MudItem>
                        <MudItem xs="4" sm="4" md="4" lg="4">
                            <MudNumericField InputMode="InputMode.numeric" Min="0" HelperText="Enter From Price" Immediate="true" Clearable="true" Label="Price" Variant="Variant.Text"
                                         Adornment="Adornment.Start" AdornmentIcon="@Icons.Material.Filled.Numbers" @bind-Value="oModelDetail.Price" TextChanged="OnChange"></MudNumericField>
                        </MudItem>
                        <MudItem xs="4" sm="4" md="4" lg="4">
                            <MudNumericField InputMode="InputMode.numeric" Min="0" HelperText="Enter From Landed Factor" Immediate="true" Clearable="true" Label="Landed Factor" Variant="Variant.Text"
                                         Adornment="Adornment.Start" AdornmentIcon="@Icons.Material.Filled.Numbers" @bind-Value="oModelDetail.LandedFactor" TextChanged="OnChange"></MudNumericField>
                        </MudItem>
                        <MudItem xs="4" sm="4" md="4" lg="4">
                            <MudNumericField InputMode="InputMode.numeric" HelperText="Cost" Immediate="true" Clearable="true" Label="Cost" Variant="Variant.Text"
                                         Adornment="Adornment.Start" AdornmentIcon="@Icons.Material.Filled.Numbers" @bind-Value="oModelDetail.Cost" Disabled></MudNumericField>
                        </MudItem>
                    </MudGrid>
                }
