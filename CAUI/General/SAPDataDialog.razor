                <RowTemplate>
                    <MudTd>@context.Currency</MudTd>
                    <MudTd>@context.Rate</MudTd>
                </RowTemplate>
                <PagerContent>
                    <MudPagination SelectedChanged="PageChanged" Count="@((_table.GetFilteredItemsCount() + _table.RowsPerPage - 1) / _table.RowsPerPage)" Class="pa-4" />
