@using CA.API.Models;

<MudDialog>
    <DialogContent>
        <MudTextField @bind-Value="Remarks" T="string" Label="Remarks" Variant="Variant.Outlined" Lines="5" Margin="Margin.Dense" />
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton OnClick="Submit">Submit</MudButton>
    </DialogActions>
</MudDialog>